{
    "name": "kreait/ezpublish-migrations-bundle",
    "description": "Migrations for eZ Publish 5",
    "keywords": ["migrations", "ezpublish"],
    "homepage": "https://github.com/kreait/ezpublish-migrations-bundle",
    "license": "MIT",
    "authors": [
        {
          "name": "Jérôme Gamez",
          "homepage": "https://github.com/jeromegamez"
        },
        {
          "name": "kreait",
          "homepage": "https://github.com/kreait"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/migrations": "1.0.0-alpha3"
    },
    "require-dev": {
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/http-kernel": "~2.3",
        "ezsystems/ezpublish-api": ">=2014.11",
        "ezsystems/ezpublish-spi": ">=2014.11",
        "phpunit/phpunit": "~4.5",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Kreait\\EzPublish\\MigrationsBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kreait\\EzPublish\\MigrationsBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
