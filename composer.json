{
    "name": "kreait/ezpublish-migrations-bundle",
    "description": "Migrations for eZ Publish 5",
    "keywords": ["migrations", "ezpublish"],
    "homepage": "https://github.com/kreait/ezpublish-migrations-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "kreait",
            "homepage": "https://github.com/kreait",
            "email": "dev@kreait.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/migrations": "~1.0@dev"
    },
    "require-dev": {
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/http-kernel": "~2.3",
        "ezsystems/ezpublish-api": "v2014.11.6",
        "ezsystems/ezpublish-spi": "v2014.11.6",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "dev-master",
        "squizlabs/php_codesniffer": "~1.5",
        "ezsystems/ezcs": "dev-master"
    },
    "autoload": {
        "psr-4": { "Kreait\\EzPublish\\MigrationsBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
